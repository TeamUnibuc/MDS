{
  "name": "barena-backend",
  "version": "1.0.0",
  "description": "NodeJS backend for BArena",
  "main": "dist/index.js",
  "scripts": {
    "build-prod": "yarn set-env-prod && tsc",
    "build": "tsc",
    "serve": "node dist/src/index.js",
    "run-dev": "yarn build && yarn serve",
    "debug": "",
    "test": "jest",
    "lint": "npx eslint .",
    "set-env-prod": "cp env/.env.prod .env",
    "set-env-dev": "cp env/.env.dev .env",
    "scripts": "grep \"scripts\" -A 50 ./package.json | sed '/}/ q' # run it on package.json directory"
  },
  "author": "Unibuc4Ever",
  "license": "UNLICENSED",
  "dependencies": {
    "@types/express": "^4.17.11",
    "express": "^4.17.1",
    "mongoose": "5.11.15",
    "passport": "^0.4.1",
    "ts-dotenv": "^0.8.3",
    "typescript": "4.1"
  },
  "devDependencies": {
    "@types/mongoose": "^5.10.3",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "eslint": "^7.20.0",
    "eslint-plugin-import-quotes": "^0.0.1",
    "eslint-plugin-no-loops": "^0.3.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.1"
  }
}
